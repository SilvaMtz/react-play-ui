{
  "name": "react-play-ui",
  "version": "0.0.16",
  "description": "Highly Interactive React.js component library",
  "bugs": {
    "url": "https://github.com/SilvaMtz/react-play-ui/issues"
  },
  "homepage": "https://github.com/SilvaMtz/react-play-ui#readme",
  "keywords": [
    "react"
  ],
  "author": "David Martinez de Silva",
  "license": "MIT",
  "main": "build/index.js",
  "module": "build/index.es.js",
  "files": [
    "build"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SilvaMtz/react-play-ui.git"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "rollup --config",
    "build-watch": "rollup -c -w",
    "start-playground": "cd docs && npm run start",
    "i-all": "npm i && cd docs && npm i",
    "dev": "npm-run-all --parallel build-watch start-playground",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "prepublishOnly": "npm run build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "@rollup/plugin-commonjs": "^17.1.0",
    "@rollup/plugin-node-resolve": "^11.1.1",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "babel-cli": "^6.26.0",
    "chroma-js": "^2.1.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.2.4",
    "rollup": "^2.38.4",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.0",
    "rollup-plugin-typescript2": "^0.29.0",
    "styled-components": "^5.2.1",
    "typescript": "^4.1.3"
  },
  "peerDependencies": {
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  },
  "dependencies": {
    "@types/react-router-dom": "^5.1.7",
    "@types/react-select": "^4.0.8",
    "react-scripts": "4.0.1",
    "react-select": "^4.0.2"
  }
}
